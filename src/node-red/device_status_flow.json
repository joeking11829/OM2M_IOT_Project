    {
        "id": "f4ba0e3c4f78972c",
        "type": "function",
        "z": "6a9fac69f8d54b14",
        "name": "Parse Single Recode from XML to JSON",
        "func": "// Check if null == payload\nif(msg.payload === \"null\")\n{\n    node.warn('the end of message seq');\n    msg.complete = true;\n    \n}else{\n    var data = JSON.parse(msg.payload);\n    var object1 = data.obj;\n    var integer1 = object1.int;\n\n    // Parse Data\n    var timestamp = integer1[0].$.val;\n    var pir = integer1[1].$.val;\n    var adc33 = integer1[2].$.val;\n\n\n    msg.payload = {\n                \"timestamp\": timestamp,\n                \"pir\": pir,\n                \"adc33\": adc33\n                };\n    \n}\n\n\nreturn msg;",
